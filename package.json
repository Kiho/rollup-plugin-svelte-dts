{
  "name": "rollup-plugin-svelte-dts",
  "version": "1.0.5",
  "description": "Create definition files from svelte JavaScript files",
  "main": "dist/rollup-plugin-svelte-dts.common.js",
  "jsnext": "dist/rollup-plugin-svelte-dts.es.js",
  "keywords": [
    "svelte",
    "rollup-plugin",
