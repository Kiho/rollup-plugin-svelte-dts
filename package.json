    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-svelte": "^3.1.0",
    "rollup-plugin-typescript2": "^0.9.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "serve": "^6.4.1",
    "svelte": "^1.41.2",
    "typescript": "^2.6.2"
